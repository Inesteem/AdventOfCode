13 read &input
z = input + 6

12 read &input
z = z*26 + input + 12

11 read &input
z = z*26 + input + 5

10 read &input
z = z*26 + input + 10

// erste entscheidung
9 read &input
if (input != (z%26)-16)
  z += input + 7
else 
  z/=26

8 read &input // 9
z += input + 7

9 read &input // 9
z += input + 4

6 read &input
if (input != (z%26)-4)
  z += input + 12
else 
  z/=26

5 read &input
z += z*26 + input + 14

4 read &input
if (input != (z%26)-7)
  z += input + 13
else 
  z/=26

